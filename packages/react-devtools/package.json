{
    "name": "@visual-dev/react-devtools",
    "version": "1.0.0",
    "description": "React DevTools SDK for Visual Dev Tool - element source locating and highlight overlay",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./babel-plugin": {
            "types": "./dist/babel-plugin/jsx-source.d.ts",
            "import": "./dist/babel-plugin/jsx-source.mjs",
            "require": "./dist/babel-plugin/jsx-source.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "lint": "eslint src --ext .ts,.tsx",
        "clean": "rm -rf dist"
    },
    "peerDependencies": {
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.24.0",
        "@babel/helper-plugin-utils": "^7.24.0",
        "@babel/types": "^7.24.0",
        "@types/babel__core": "^7.20.5",
        "@types/babel__helper-plugin-utils": "^7.10.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "keywords": [
        "react",
        "devtools",
        "visual-dev",
        "element-inspector"
    ],
    "license": "MIT"
}